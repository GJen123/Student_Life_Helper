

@model List<Project.Models.MoneyViewModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container">
    <table class="table table-hover table-striped">
        <tr>
            <th>月份</th>
            <th>金錢使用狀況</th>
            

        </tr>

        @foreach (var record in Model)
        {
            <tr>
                <td>@record.Month</td>
                <td>@record.Balance</td>
               


            </tr>
        }
    </table>

    <div class="col-xs-4" style="text-align:center;font-style:italic">
        
        <p><a class="btn btn-info btn-lg mybtn5" href="~/manage/Chichen_Lee">財務報表 &raquo;</a></p>
    </div>

</div>